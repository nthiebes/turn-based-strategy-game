{
  // Details: https://github.com/victorporof/Sublime-JSHint#using-your-own-jshintrc-options
  // Example: https://github.com/jshint/jshint/blob/master/examples/.jshintrc
  // Documentation: http://www.jshint.com/docs/p
  "browser": true,
  "esnext": true,
  "globals": {
      "booky": true,
      "$": true,
      "userOptions": true,
      "isMobile": true,
      "window": true,
      "document": true,
      "requestTimeout": true,
      "location": true,
      "unescape": true,
      "FastClick": true,
      "FileReader": true,
      "ga": true,
      "describe": true,
      "it": true,
      "expect": true,
      "beforeEach": true,
      "ajax": true,
      "Booky": true,
      "chrome": true,
      "rd": true,
      "requestAnimFrame": true
  },
  "onevar": true,
  "curly" : true,
  "noarg" : true,
  "globalstrict": false,
  "quotmark": "single",
  "smarttabs": false,
  "trailing": false,
  "undef": true,
  "unused": true,
  "maxdepth": 3,
  "nonew" : true,
  "noempty" : true,
  "newcap" : true,
  "bitwise" : true,
  "forin" : true,
  "camelcase" : true,
  "latedef" : true,
  "immed" : true,
  "maxparams": 4,
  "eqeqeq": true,
  "jasmine": true,
  "sub": false,
  "maxcomplexity": 10,
  "shadow": "outer",
  "maxlen": 200,
  "freeze": true
}
